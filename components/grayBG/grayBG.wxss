.grayBG {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100vh;
  background-color: #32323280;
  z-index: 10;
}

.grayBG-window {
  position: relative;
  width: 634rpx;
  /* margin: 0 auto; */
  border-radius: 12rpx;
  background-color: #fff;
  padding-bottom: 40rpx;
}

.grayBG-window-close {
  position: absolute;
  right: 25rpx;
  top: 25rpx;
  width: 42rpx;
  height: 42rpx;
}